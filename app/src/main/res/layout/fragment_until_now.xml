<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/project_white">

        <LinearLayout
            android:id="@+id/main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/project_white"
            android:orientation="vertical"
            tools:context=".view.board.write.fragment_write_new_board_preview">

            <include
                android:id="@+id/util_meet1"
                layout="@layout/list_item_util_now"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/util_meet2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                layout="@layout/list_item_util_now"/>

            <include
                android:id="@+id/util_meet3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                layout="@layout/list_item_util_now"/>
        </LinearLayout>
    </ScrollView>
</layout>
